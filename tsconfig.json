{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": false,
    "outDir": "lib",
    "rootDir": "src",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "jsx": "react",
    "reactNamespace": "E",
    "resolveJsonModule": true,
    "lib": ["ESNext", "dom",]
  }
}
